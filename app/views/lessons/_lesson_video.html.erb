<div class = 'lesson-video-container'>
  
  <% if video.signed_in_only? %>

    <h2>Tylko dla zalogowanych użytkowników</h2>
    <h3>
      <%= link_to "Zaloguj się", new_user_session_path %>
      lub 
      <%= link_to "Zarejestruj się", new_user_registration_path %>
      aby obejrzeć.
    </h3>
  
  <% else %>


	<iframe class = 'lesson-video' 
	  src = <%= video.embed_link %> 
    id = <%= "video-#{video.id}" %>
	  allowfullscreen >

  <% end %>


</iframe>
</div>