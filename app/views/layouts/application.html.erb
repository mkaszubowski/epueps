<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield :title) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script type="text/javascript"
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAXVNHYE0uUr08vL7ycXMYLrQ7PTjK-KDI">
  </script>
  <%= csrf_meta_tags %>
</head>
<body class = "<%="#{controller_name.dasherize}-controller #{action_name.dasherize}" %>" >

  <%= render 'layouts/header' %>

  <%= yield %>

  <% unless @splash || no_container?(yield(:no_container)) %>
    </div>
  <% end %>

  <div class="params-debug">
    <%= debug(params) if Rails.env == 'development' %>
  </div>

  <%= render 'layouts/footer' %>

</body>
</html>
